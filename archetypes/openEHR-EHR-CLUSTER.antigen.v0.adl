definition
	CLUSTER[at0000] matches {    -- Antigen
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0014] matches {    -- Code
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Locus
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- HLA-A
							at0003,    -- HLA-B
							at0004,    -- HLA-C
							at0005,    -- HLA-DR
							at0006,    -- HLA-DQA
							at0007,    -- HLA-DQB
							at0008,    -- HLA-DPA
							at0009]    -- HLA-DPB
						}
					}
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {    -- Type
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- Method
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0012,    -- DNA
							at0013]    -- Serology
						}
					}
				}
			}
		}
	}

